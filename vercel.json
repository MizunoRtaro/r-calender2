{
    "builds": [
      {
        "src": "index.html",
        "use": "@vercel/static",
        "config": {
          "zeroConfig": true
        }
      }
    ],
    "routes": [
      {
        "src": "/(.*)",
        "dest": "/index.html"
      }
    ],
    "build": {
      "env": {
        "CLIENT_ID": "@client_id",
        "SPREADSHEET_ID": "@spreadsheet_id"
      }
    }
  }